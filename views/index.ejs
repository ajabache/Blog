<div class="author">
    <h1 class="author_heading">Hi, I am AJ</h1>
    <p class="author_body">Web Developer</p>
</div>

<img src="/img/car.jpg" alt="author_image" class="author_image" width="981" height="528">

<section class="articles">
    <h2 class="articles_heading">Latest Post</h2>

    <ul class="article_ul">
        <% data.forEach(post => { %>
            <li>
                <a href="/post/<%= post._id %>">
                    <span><%= post.title %></span>
                    <span class="article_list_date"><%= post.createdAt.toDateString() %></span>
                </a>
            </li>
        <% }) %>
    </ul>

    <% if (nextPage !== null) { %>
        <a href="/?page=<%= nextPage %>" class="pagination">&lt; View Older Posts</a>
    <% } %>

    
</section>